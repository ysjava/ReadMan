<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.learn">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.learn.ipctestserver.CUSTOM_PERMISSION" /> <!-- 安卓11访问其他程序需要appid -->
    <queries>
        <package android:name="com.learn.ipctestserver" />
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Nnsman">
        <activity
            android:name=".imageviewpingpu.IamgeViewPingpuActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".file.FileActivity"
            android:exported="true"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".span.SpanActivity"
            android:exported="true" />
        <activity
            android:name=".ipc.IpcActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".ipc.IpcTest2Activity"
            android:process=":remote" />
        <activity
            android:name=".ipc.IpcTest3Activity"
            android:process="com.android.learn.ipc.remote" />
        <activity
            android:name=".window.WindowActivity"
            android:exported="true" />

        <service
            android:name=".ipc.MyService"
            android:process=":remote" />
        <service
            android:name=".ipc.BookManagerService"
            android:process=":remote.bookmanager" />
    </application>

</manifest>